{
  "name": "websets-clone",
  "version": "1.0.0",
  "description": "Websets clone with 2 REST APIs and full frontend",
  "main": "simple-api.js",
  "scripts": {
    "start": "node simple-api.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node simple-api.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 